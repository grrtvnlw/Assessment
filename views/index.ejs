<%- include('partials/head') %>

<% for(var i=0; i < results.length; i++) { %>
  <ul>
    <li><%= results[i].invoice_number %></li>
    <li><%= results[i].vendor_name %></li>
    <li><%= results[i].remittance_address %></li>
    <li><%= results[i].total %></li>
    <li><%= results[i].invoice_date %></li>
    <li><%= results[i].due_date %></li>
    <li>
      <label for="completed">Completed</label>
      <input type="checkbox" name="completed" class="completed" id="<%= results[i].invoice_number %>">
    </li>
  </ul>
  <% } %>

<%- include('partials/foot') %>